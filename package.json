  "author": "osedhelu",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osedhelu/BackendTypeScript.git"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mssql": "^6.0.7",
    "@types/node": "^14.14.41",
    "@types/socket.io": "^2.1.13",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4",
    "typescript-tslint-plugin": "^1.0.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
